#include <vcl.h>
#pragma hdrstop

#include "mad_kalmi.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm4 *Form4;
//---------------------------------------------------------------------------
__fastcall TForm4::TForm4(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm4::p1MouseLeave(TObject *Sender)
{
p1->Color=clWhite;
   p1->Font->Color=0x0085A016;

}
//---------------------------------------------------------------------------

void __fastcall TForm4::p1MouseMove(TObject *Sender, TShiftState Shift, int X, int Y)

{
     p1->Color=0x0085A016;
	p1->Font->Color=clWhite;

}
