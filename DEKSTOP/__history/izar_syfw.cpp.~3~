#include <vcl.h>
#pragma hdrstop

#include "izar_syfw.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm8 *Form8;
//---------------------------------------------------------------------------
__fastcall TForm8::TForm8(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------

void __fastcall TForm8::p1MouseLeave(TObject *Sender)
{
p1->Color=clWhite;
   p1->Font->Color=0x0085A016;

}
//---------------------------------------------------------------------------

void __fastcall TForm8::p1MouseMove(TObject *Sender, TShiftState Shift, int X, int Y)

{
 p1->Color=0x0085A016;
	p1->Font->Color=clWhite;

}
//---------------------------------------------------------------------------

